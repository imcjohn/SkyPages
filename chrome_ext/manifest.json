{
  "manifest_version": 2,

  "name": "SkyPages Plugin",
  "description": "This extension allows you to upload the current chrome webpage to SkyNet through SkyPages.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["bg_page.js","gitTraverse.js","ziphandler.js","jszip.min.js"],
    "persistent": true
  },
  "permissions": [
    "activeTab",
    "tabs"
  ]
}